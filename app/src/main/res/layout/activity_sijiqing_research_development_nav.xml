<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".navigation.SijiqingResearchDevelopmentNavActivity">

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/sijiqing_research_development_nav_guideline7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.9" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/sijiqing_research_development_nav_guideline6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.1" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/sijiqing_research_development_nav_guideline4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.75" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/sijiqing_research_development_nav_guideline2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.5" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/sijiqing_research_development_nav_guideline"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.25" />

    <Button
        android:id="@+id/sijiqing_research_development_income_nav"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="进料数据现场记录表"
        app:layout_constraintBottom_toTopOf="@+id/sijiqing_research_development_nav_guideline"
        app:layout_constraintEnd_toStartOf="@+id/sijiqing_research_development_nav_guideline7"
        app:layout_constraintStart_toStartOf="@+id/sijiqing_research_development_nav_guideline6"
        app:layout_constraintTop_toTopOf="@+id/sijiqing_research_development_nav_guideline" />

    <Button
        android:id="@+id/sijiqing_research_development_outcome_nav"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="出料数据现场记录表"
        app:layout_constraintBottom_toTopOf="@+id/sijiqing_research_development_nav_guideline2"
        app:layout_constraintEnd_toStartOf="@+id/sijiqing_research_development_nav_guideline7"
        app:layout_constraintStart_toStartOf="@+id/sijiqing_research_development_nav_guideline6"
        app:layout_constraintTop_toTopOf="@+id/sijiqing_research_development_nav_guideline2" />

    <Button
        android:id="@+id/sijiqing_research_development_temp_hum_nav"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="温湿度汇总表"
        app:layout_constraintBottom_toTopOf="@+id/sijiqing_research_development_nav_guideline4"
        app:layout_constraintEnd_toStartOf="@+id/sijiqing_research_development_nav_guideline7"
        app:layout_constraintStart_toStartOf="@+id/sijiqing_research_development_nav_guideline6"
        app:layout_constraintTop_toTopOf="@+id/sijiqing_research_development_nav_guideline4" />

</androidx.constraintlayout.widget.ConstraintLayout>