<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".householdWaste.AbnormalRubbishActivity">

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/comment_label"
        android:textSize="24sp"
        app:layout_constraintBottom_toTopOf="@+id/abnormal_rubbish_guideline28"
        app:layout_constraintStart_toStartOf="@+id/abnormal_rubbish_guideline31"
        app:layout_constraintTop_toTopOf="@+id/abnormal_rubbish_guideline28" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="数量"
        android:textSize="24sp"
        app:layout_constraintBottom_toTopOf="@+id/abnormal_rubbish_guideline26"
        app:layout_constraintStart_toStartOf="@+id/abnormal_rubbish_guideline31"
        app:layout_constraintTop_toTopOf="@+id/abnormal_rubbish_guideline26" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="垃圾种类"
        android:textSize="24sp"
        app:layout_constraintBottom_toTopOf="@+id/abnormal_rubbish_guideline23"
        app:layout_constraintStart_toStartOf="@+id/abnormal_rubbish_guideline31"
        app:layout_constraintTop_toTopOf="@+id/abnormal_rubbish_guideline23" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="垃圾来源"
        android:textSize="24sp"
        app:layout_constraintBottom_toTopOf="@+id/abnormal_rubbish_guideline25"
        app:layout_constraintStart_toStartOf="@+id/abnormal_rubbish_guideline31"
        app:layout_constraintTop_toTopOf="@+id/abnormal_rubbish_guideline25" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/abnormal_rubbish_guideline23"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.49" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/abnormal_rubbish_guideline24"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.25" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/abnormal_rubbish_guideline25"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.37" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/abnormal_rubbish_guideline26"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.61" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/abnormal_rubbish_guideline27"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.9" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/abnormal_rubbish_guideline28"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.73" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/abnormal_rubbish_guideline29"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.1" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="异常垃圾情况登记表"
        android:textSize="30sp"
        app:layout_constraintBottom_toTopOf="@+id/abnormal_rubbish_guideline29"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/abnormal_rubbish_guideline29" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/abnormal_rubbish_guideline30"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.9" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/abnormal_rubbish_guideline31"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.1" />

    <EditText
        android:id="@+id/abnormal_rubbish_carNum"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="textPersonName"
        app:layout_constraintBottom_toTopOf="@+id/abnormal_rubbish_guideline24"
        app:layout_constraintEnd_toStartOf="@+id/abnormal_rubbish_guideline30"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/textView"
        app:layout_constraintTop_toTopOf="@+id/abnormal_rubbish_guideline24"
        app:layout_constraintVertical_bias="0.511" />

    <EditText
        android:id="@+id/abnormal_rubbish_rubbish_src"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="textPersonName"
        app:layout_constraintBottom_toTopOf="@+id/abnormal_rubbish_guideline25"
        app:layout_constraintEnd_toStartOf="@+id/abnormal_rubbish_guideline30"
        app:layout_constraintStart_toEndOf="@+id/textView2"
        app:layout_constraintTop_toTopOf="@+id/abnormal_rubbish_guideline25" />

    <EditText
        android:id="@+id/abnormal_rubbish_rubbish_kind"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="textPersonName"
        app:layout_constraintBottom_toTopOf="@+id/abnormal_rubbish_guideline23"
        app:layout_constraintEnd_toStartOf="@+id/abnormal_rubbish_guideline30"
        app:layout_constraintStart_toEndOf="@+id/textView3"
        app:layout_constraintTop_toTopOf="@+id/abnormal_rubbish_guideline23" />

    <EditText
        android:id="@+id/abnormal_rubbish_weight"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="textPersonName"
        app:layout_constraintBottom_toTopOf="@+id/abnormal_rubbish_guideline26"
        app:layout_constraintEnd_toStartOf="@+id/abnormal_rubbish_weight_btn"
        app:layout_constraintStart_toEndOf="@+id/textView4"
        app:layout_constraintTop_toTopOf="@+id/abnormal_rubbish_guideline26" />

    <EditText
        android:id="@+id/abnormal_rubbish_comment"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="textPersonName"
        app:layout_constraintBottom_toTopOf="@+id/abnormal_rubbish_guideline28"
        app:layout_constraintEnd_toStartOf="@+id/abnormal_rubbish_guideline30"
        app:layout_constraintStart_toEndOf="@+id/textView5"
        app:layout_constraintTop_toTopOf="@+id/abnormal_rubbish_guideline28" />

    <Button
        android:id="@+id/abnormal_rubbish_save"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="@string/save_label"
        android:textSize="24sp"
        app:layout_constraintBottom_toTopOf="@+id/abnormal_rubbish_guideline27"
        app:layout_constraintEnd_toStartOf="@+id/abnormal_rubbish_guideline30"
        app:layout_constraintStart_toStartOf="@+id/abnormal_rubbish_guideline31"
        app:layout_constraintTop_toTopOf="@+id/abnormal_rubbish_guideline27" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="车号"
        android:textSize="24sp"
        app:layout_constraintBottom_toTopOf="@+id/abnormal_rubbish_guideline24"
        app:layout_constraintStart_toStartOf="@+id/abnormal_rubbish_guideline31"
        app:layout_constraintTop_toTopOf="@+id/abnormal_rubbish_guideline24" />

    <Button
        android:id="@+id/abnormal_rubbish_weight_btn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/chengzhong"
        android:textSize="24sp"
        app:layout_constraintBottom_toTopOf="@+id/abnormal_rubbish_guideline26"
        app:layout_constraintEnd_toStartOf="@+id/abnormal_rubbish_guideline30"
        app:layout_constraintTop_toTopOf="@+id/abnormal_rubbish_guideline26" />
</androidx.constraintlayout.widget.ConstraintLayout>