<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".navigation.KitchenRubbishNavActivity">

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/kitchen_rubbish_nav_guideline16"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.17" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/kitchen_rubbish_nav_guideline17"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.33" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/kitchen_rubbish_nav_guideline18"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.5" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/kitchen_rubbish_nav_guideline19"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.67" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/kitchen_rubbish_nav_guideline20"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.8358413" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/kitchen_rubbish_nav_guideline21"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.1" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/kitchen_rubbish_nav_guideline22"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.9" />

    <Button
        android:id="@+id/worm_incoming_nav"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="来料记录表"
        android:textSize="24sp"
        app:layout_constraintBottom_toTopOf="@+id/kitchen_rubbish_nav_guideline16"
        app:layout_constraintEnd_toStartOf="@+id/kitchen_rubbish_nav_guideline22"
        app:layout_constraintStart_toStartOf="@+id/kitchen_rubbish_nav_guideline21"
        app:layout_constraintTop_toTopOf="@+id/kitchen_rubbish_nav_guideline16" />

    <Button
        android:id="@+id/worm_outcoming_nav"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="出料记录表"
        android:textSize="24sp"
        app:layout_constraintBottom_toTopOf="@+id/kitchen_rubbish_nav_guideline17"
        app:layout_constraintEnd_toStartOf="@+id/kitchen_rubbish_nav_guideline22"
        app:layout_constraintStart_toStartOf="@+id/kitchen_rubbish_nav_guideline21"
        app:layout_constraintTop_toTopOf="@+id/kitchen_rubbish_nav_guideline17" />

    <Button
        android:id="@+id/worm_temp_nav"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="温度记录表"
        android:textSize="24sp"
        app:layout_constraintBottom_toTopOf="@+id/kitchen_rubbish_nav_guideline18"
        app:layout_constraintEnd_toStartOf="@+id/kitchen_rubbish_nav_guideline22"
        app:layout_constraintStart_toStartOf="@+id/kitchen_rubbish_nav_guideline21"
        app:layout_constraintTop_toTopOf="@+id/kitchen_rubbish_nav_guideline18" />

    <Button
        android:id="@+id/worm_hatching_nav"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="虫卵孵化记录表"
        android:textSize="24sp"
        app:layout_constraintBottom_toTopOf="@+id/kitchen_rubbish_nav_guideline19"
        app:layout_constraintEnd_toStartOf="@+id/kitchen_rubbish_nav_guideline22"
        app:layout_constraintStart_toStartOf="@+id/kitchen_rubbish_nav_guideline21"
        app:layout_constraintTop_toTopOf="@+id/kitchen_rubbish_nav_guideline19" />

    <Button
        android:id="@+id/worm_saling_nav"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="各场站出料成虫、虫粪外销情况统计表"
        android:textSize="24sp"
        app:layout_constraintBottom_toTopOf="@+id/kitchen_rubbish_nav_guideline20"
        app:layout_constraintEnd_toStartOf="@+id/kitchen_rubbish_nav_guideline22"
        app:layout_constraintStart_toStartOf="@+id/kitchen_rubbish_nav_guideline21"
        app:layout_constraintTop_toTopOf="@+id/kitchen_rubbish_nav_guideline20" />

</androidx.constraintlayout.widget.ConstraintLayout>