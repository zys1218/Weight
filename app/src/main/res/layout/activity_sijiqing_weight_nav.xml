<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".navigation.SijiqingWeightNavActivity">

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/sijiqing_weight_nav_guideline4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.9" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/sijiqing_weight_nav_guideline3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.1" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/sijiqing_weight_nav_guideline2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.66" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/sijiqing_weight_nav_guideline"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.33" />

    <Button
        android:id="@+id/sijiqing_weight_shit_nav"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="粪便记录表"
        app:layout_constraintBottom_toTopOf="@+id/sijiqing_weight_nav_guideline"
        app:layout_constraintEnd_toStartOf="@+id/sijiqing_weight_nav_guideline4"
        app:layout_constraintStart_toStartOf="@+id/sijiqing_weight_nav_guideline3"
        app:layout_constraintTop_toTopOf="@+id/sijiqing_weight_nav_guideline" />

    <Button
        android:id="@+id/sijiqing_weight_kitchen_rubbish_nav"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="厨余垃圾记录表"
        app:layout_constraintBottom_toTopOf="@+id/sijiqing_weight_nav_guideline2"
        app:layout_constraintEnd_toStartOf="@+id/sijiqing_weight_nav_guideline4"
        app:layout_constraintStart_toStartOf="@+id/sijiqing_weight_nav_guideline3"
        app:layout_constraintTop_toTopOf="@+id/sijiqing_weight_nav_guideline2" />

</androidx.constraintlayout.widget.ConstraintLayout>